{
  "module": "sdl2_mixer",
  "about": "SDL2_mixer using codec modules (vorbis/opus/mpg123/xmp/gme)",
  "author": "djokersoft",
  "version": "1.0.0",
  "depends": [
    "sdl2",
    "ogg",
    "vorbis",
    "opus",
    "mpg123",
    "xmp",
    "gme"
  ],
  "static": true,
  "priority": 0,
  "system": [
    "linux",
    "android",
    "windows"
  ],
  "src": [
    "src/mixer.c",
    "src/music.c",
    "src/utils.c",
    "src/effects_internal.c",
    "src/effect_position.c",
    "src/effect_stereoreverse.c",
    "src/codecs/load_aiff.c",
    "src/codecs/load_voc.c",
    "src/codecs/mp3utils.c",
    "src/codecs/music_wav.c",
    "src/codecs/music_ogg.c",
    "src/codecs/music_opus.c",
    "src/codecs/music_mpg123.c",
    "src/codecs/music_xmp.c",
    "src/codecs/music_gme.c",
    "src/codecs/music_drflac.c"
  ],
  "include": [
    "src",
    "src/codecs"
  ],
  "plataforms": {
    "linux": {
      "CPP_ARGS": "",
      "CC_ARGS": "-DMUSIC_WAV -DMUSIC_OGG -DMUSIC_OPUS -DMUSIC_MP3_MPG123 -DMUSIC_MOD_XMP -DMUSIC_GME -DMUSIC_FLAC_DRFLAC",
      "LD_ARGS": "",
      "src": [],
      "include": []
    },
    "windows": {
      "CPP_ARGS": "",
      "CC_ARGS": "-DMUSIC_WAV -DMUSIC_OGG -DMUSIC_OPUS -DMUSIC_MP3_MPG123 -DMUSIC_MOD_XMP -DMUSIC_GME -DMUSIC_FLAC_DRFLAC",
      "LD_ARGS": "",
      "src": [],
      "include": []
    },
    "android": {
      "CPP_ARGS": "",
      "CC_ARGS": "-DMUSIC_WAV -DMUSIC_OGG -DMUSIC_OPUS -DMUSIC_MP3_MPG123 -DMUSIC_MOD_XMP -DMUSIC_GME -DMUSIC_FLAC_DRFLAC",
      "LD_ARGS": "",
      "src": [],
      "include": []
    }
  }
}
